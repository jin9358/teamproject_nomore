<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.korit.nomoreback.domain.moim.MoimBanMapper">

    <insert id="insertBan">
        INSERT INTO moim_ban_tb (moim_id, user_id)
        VALUES (#{moimId}, #{userId})
    </insert>

    <select id="selectBanUser" resultType="com.korit.nomoreback.domain.moim.MoimBan">
        select
            *
        from
            moim_ban_tb
        where
            moim_id = #{moimId}
    </select>

</mapper>
